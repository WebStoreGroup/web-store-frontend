{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "max-len": ["error", 80],
    "import/no-extraneous-dependencies": 0,
    "react/forbid-prop-types": ["error", { "forbid": ["any"] }]
  },
  "env": {
    "browser": true
  }
}
